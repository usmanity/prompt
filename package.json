    "@vue/cli-service": "3.12.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.12.1",
    "eslint-plugin-vue": "5.2.3",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
